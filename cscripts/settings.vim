"My settings
syntax on
filetype plugin on
set omnifunc=syntaxcomplete#Complete
set clipboard+=unnamed
colorscheme dracula
set hidden
set number
set relativenumber
set modeline

"netrw variables
let g:netrw_banner = 0
let g:netrw_browse_split = 4
let g:netrw_winsize = 25

"Tab settings
set expandtab
set tabstop=2
set softtabstop=2
set shiftwidth=2
